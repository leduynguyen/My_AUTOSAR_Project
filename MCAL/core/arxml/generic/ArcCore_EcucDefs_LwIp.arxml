      <AR-PACKAGES>
        <AR-PACKAGE UUID="4cfb9d7b-9824-4fd6-93b3-7f883571203a">
          <SHORT-NAME>EcucDefs</SHORT-NAME>
          <AR-PACKAGES>
            <AR-PACKAGE UUID="e9694964-70f8-4039-9c2e-7f6e7a523280">
              <SHORT-NAME>Generic</SHORT-NAME>
              <ELEMENTS>
                <ECUC-MODULE-DEF UUID="ECUC:8fc09477-4d31-58bf-9fcc-0f2e51345aad">
                  <SHORT-NAME>LwIp</SHORT-NAME>
                  <DESC>
                    <L-2 L="EN">Configuration of the LwIp module</L-2>
                  </DESC>
                  <ADMIN-DATA>
                    <DOC-REVISIONS>
                      <DOC-REVISION>
                        <REVISION-LABEL />
                        <ISSUED-BY>ARCCORE</ISSUED-BY>
                        <DATE>2013-06-14</DATE>
                      </DOC-REVISION>
                    </DOC-REVISIONS>
                  </ADMIN-DATA>
                  <LOWER-MULTIPLICITY>0</LOWER-MULTIPLICITY>
                  <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                  <SUPPORTED-CONFIG-VARIANTS>
                    <SUPPORTED-CONFIG-VARIANT>VARIANT-PRE-COMPILE</SUPPORTED-CONFIG-VARIANT>
                  </SUPPORTED-CONFIG-VARIANTS>
                  <CONTAINERS>
                    <ECUC-PARAM-CONF-CONTAINER-DEF UUID="ECUC:022760ec-ae2e-5125-96ad-ca206d355f96">
                      <SHORT-NAME>LwIpGeneral</SHORT-NAME>
                      <DESC>
                        <L-2 L="EN">Configuration of DHCP and AutoIP options</L-2>
                      </DESC>
                      <LOWER-MULTIPLICITY>1</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                      <MULTIPLE-CONFIGURATION-CONTAINER>false</MULTIPLE-CONFIGURATION-CONTAINER>
                      <PARAMETERS>
                        <ECUC-BOOLEAN-PARAM-DEF UUID="ECUC:b34cdb52-707b-5af1-8403-52611cb0f7b2">
                          <SHORT-NAME>LwIpDhcp</SHORT-NAME>
                          <DESC>
                            <L-2 L="EN">This parameter indicates whether DHCP is used or not</L-2>
                          </DESC>
                          <LOWER-MULTIPLICITY>1</LOWER-MULTIPLICITY>
                          <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                          <IMPLEMENTATION-CONFIG-CLASSES>
                            <ECUC-IMPLEMENTATION-CONFIGURATION-CLASS>
                              <CONFIG-CLASS>PRE-COMPILE</CONFIG-CLASS>
                              <CONFIG-VARIANT>VARIANT-PRE-COMPILE</CONFIG-VARIANT>
                            </ECUC-IMPLEMENTATION-CONFIGURATION-CLASS>
                          </IMPLEMENTATION-CONFIG-CLASSES>
                          <ORIGIN>ARC_ECUC</ORIGIN>
                          <SYMBOLIC-NAME-VALUE>false</SYMBOLIC-NAME-VALUE>
                          <DEFAULT-VALUE>1</DEFAULT-VALUE>
                        </ECUC-BOOLEAN-PARAM-DEF>
                        <ECUC-BOOLEAN-PARAM-DEF UUID="f5c3e0b4-74a4-4a3e-ad76-87b01ab820ee">
                          <SHORT-NAME>LwIpAutoIp</SHORT-NAME>
                          <DESC>
                            <L-2 L="EN">This parameter indicates whether AutoIP is used or not</L-2>
                          </DESC>
                          <LOWER-MULTIPLICITY>1</LOWER-MULTIPLICITY>
                          <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                          <IMPLEMENTATION-CONFIG-CLASSES>
                            <ECUC-IMPLEMENTATION-CONFIGURATION-CLASS>
                              <CONFIG-CLASS>PRE-COMPILE</CONFIG-CLASS>
                              <CONFIG-VARIANT>VARIANT-PRE-COMPILE</CONFIG-VARIANT>
                            </ECUC-IMPLEMENTATION-CONFIGURATION-CLASS>
                          </IMPLEMENTATION-CONFIG-CLASSES>
                          <ORIGIN>ARC_ECUC</ORIGIN>
                          <SYMBOLIC-NAME-VALUE>false</SYMBOLIC-NAME-VALUE>
                          <DEFAULT-VALUE>0</DEFAULT-VALUE>
                        </ECUC-BOOLEAN-PARAM-DEF>
                        <ECUC-STRING-PARAM-DEF UUID="658164a7-7053-453d-9ef9-21a490f1389f">
                          <SHORT-NAME>LwIpIPAddress</SHORT-NAME>
                          <DESC>
                            <L-2 L="EN">This parameter sets the IP address e.g. 192.168.0.5</L-2>
                          </DESC>
                          <LOWER-MULTIPLICITY>1</LOWER-MULTIPLICITY>
                          <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                          <IMPLEMENTATION-CONFIG-CLASSES>
                            <ECUC-IMPLEMENTATION-CONFIGURATION-CLASS>
                              <CONFIG-CLASS>PRE-COMPILE</CONFIG-CLASS>
                              <CONFIG-VARIANT>VARIANT-PRE-COMPILE</CONFIG-VARIANT>
                            </ECUC-IMPLEMENTATION-CONFIGURATION-CLASS>
                          </IMPLEMENTATION-CONFIG-CLASSES>
                        </ECUC-STRING-PARAM-DEF>
                        <ECUC-STRING-PARAM-DEF UUID="ce5ac35f-0840-41cd-a592-74ed66e236c4">
                          <SHORT-NAME>LwIpNetmask</SHORT-NAME>
                          <DESC>
                            <L-2 L="EN">This parameter sets the netmask e.g. 255.255.255.0</L-2>
                          </DESC>
                          <LOWER-MULTIPLICITY>1</LOWER-MULTIPLICITY>
                          <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                          <IMPLEMENTATION-CONFIG-CLASSES>
                            <ECUC-IMPLEMENTATION-CONFIGURATION-CLASS>
                              <CONFIG-CLASS>PRE-COMPILE</CONFIG-CLASS>
                              <CONFIG-VARIANT>VARIANT-PRE-COMPILE</CONFIG-VARIANT>
                            </ECUC-IMPLEMENTATION-CONFIGURATION-CLASS>
                          </IMPLEMENTATION-CONFIG-CLASSES>
                        </ECUC-STRING-PARAM-DEF>
                        <ECUC-STRING-PARAM-DEF UUID="81e6647d-e957-4253-bb5c-61dbe9318b2a">
                          <SHORT-NAME>LwIpEthernetMacAddress</SHORT-NAME>
                          <DESC>
                            <L-2 L="EN">This parameter sets the Ethernet MAC address. E.g. 00-00-00-00-00-06</L-2>
                          </DESC>
                          <LOWER-MULTIPLICITY>1</LOWER-MULTIPLICITY>
                          <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                          <IMPLEMENTATION-CONFIG-CLASSES>
                            <ECUC-IMPLEMENTATION-CONFIGURATION-CLASS>
                              <CONFIG-CLASS>PRE-COMPILE</CONFIG-CLASS>
                              <CONFIG-VARIANT>VARIANT-PRE-COMPILE</CONFIG-VARIANT>
                            </ECUC-IMPLEMENTATION-CONFIGURATION-CLASS>
                          </IMPLEMENTATION-CONFIG-CLASSES>
                        </ECUC-STRING-PARAM-DEF>
                        <ECUC-STRING-PARAM-DEF UUID="f48c26da-0daa-4a7d-a367-b875e22aeebc">
                          <SHORT-NAME>LwIpGateway</SHORT-NAME>
                          <DESC>
                            <L-2 L="EN">This parameter sets the gateway address e.g. 192.168.0.1</L-2>
                          </DESC>
                          <LOWER-MULTIPLICITY>1</LOWER-MULTIPLICITY>
                          <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                          <IMPLEMENTATION-CONFIG-CLASSES>
                            <ECUC-IMPLEMENTATION-CONFIGURATION-CLASS>
                              <CONFIG-CLASS>PRE-COMPILE</CONFIG-CLASS>
                              <CONFIG-VARIANT>VARIANT-PRE-COMPILE</CONFIG-VARIANT>
                            </ECUC-IMPLEMENTATION-CONFIGURATION-CLASS>
                          </IMPLEMENTATION-CONFIG-CLASSES>
                        </ECUC-STRING-PARAM-DEF>
                      </PARAMETERS>
                    </ECUC-PARAM-CONF-CONTAINER-DEF>
                    <ECUC-PARAM-CONF-CONTAINER-DEF UUID="ECUC:022760ec-ae2e-5125-96ad-ca206d355f96">
                      <SHORT-NAME>LwIpAdvanced</SHORT-NAME>
                      <DESC>
                        <L-2 L="EN">Advanced memory management</L-2>
                      </DESC>
                      <LOWER-MULTIPLICITY>1</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                      <MULTIPLE-CONFIGURATION-CONTAINER>false</MULTIPLE-CONFIGURATION-CONTAINER>
                      <PARAMETERS>
                        <ECUC-INTEGER-PARAM-DEF UUID="d8151586-625b-4e0a-913e-444a29901186">
                          <SHORT-NAME>LwIpMempNumNetConn</SHORT-NAME>
                          <DESC>
                            <L-2>MEMP_NUM_NETCONN: the number of struct netconns. This must be increased if you add more sockets then default is supported by config.</L-2>
                          </DESC>
                          <LOWER-MULTIPLICITY>1</LOWER-MULTIPLICITY>
                          <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                          <DEFAULT-VALUE>6</DEFAULT-VALUE>
                        </ECUC-INTEGER-PARAM-DEF>
                        <ECUC-INTEGER-PARAM-DEF UUID="d8151586-625b-4e0a-913e-444a29901186">
                          <SHORT-NAME>LwIpPoolSize</SHORT-NAME>
                          <DESC>
                            <L-2>PBUF_POOL_SIZE: the number of buffers in the pbuf pool.</L-2>
                          </DESC>
                          <LOWER-MULTIPLICITY>1</LOWER-MULTIPLICITY>
                          <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                          <DEFAULT-VALUE>10</DEFAULT-VALUE>
                        </ECUC-INTEGER-PARAM-DEF>
                        <ECUC-INTEGER-PARAM-DEF UUID="d8151586-625b-4e0a-913e-444a29901186">
                          <SHORT-NAME>LwIpMemSize</SHORT-NAME>
                          <DESC>
                            <L-2>MEM_SIZE: the size of the heap memory. If the application will send a lot of data that needs to be copied, this should be set high.</L-2>
                          </DESC>
                          <LOWER-MULTIPLICITY>1</LOWER-MULTIPLICITY>
                          <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                          <DEFAULT-VALUE>1600</DEFAULT-VALUE>
                        </ECUC-INTEGER-PARAM-DEF>
                        <ECUC-INTEGER-PARAM-DEF UUID="d8151586-625b-4e0a-913e-444a29901186">
                          <SHORT-NAME>LwIpMempNumPBuf</SHORT-NAME>
                          <DESC>
                            <L-2>MEMP_NUM_PBUF: the number of memp struct pbufs. If the application sends a lot of data out of ROM (or other static memory), this should be set high.</L-2>
                          </DESC>
                          <LOWER-MULTIPLICITY>1</LOWER-MULTIPLICITY>
                          <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                          <DEFAULT-VALUE>10</DEFAULT-VALUE>
                        </ECUC-INTEGER-PARAM-DEF>
                      </PARAMETERS>
                    </ECUC-PARAM-CONF-CONTAINER-DEF>
                  </CONTAINERS>
                </ECUC-MODULE-DEF>
              </ELEMENTS>
              <AR-PACKAGES>
                <AR-PACKAGE UUID="3da8d86d-1062-4c5d-8330-507ae1f48302">
                  <SHORT-NAME>Implementations</SHORT-NAME>
                  <ELEMENTS>
                    <BSW-IMPLEMENTATION UUID="cb640fd5-c8eb-45a8-8443-99cd847de1d5">
                      <SHORT-NAME>LwIp</SHORT-NAME>
                      <PROGRAMMING-LANGUAGE>C</PROGRAMMING-LANGUAGE>
                      <SW-VERSION>1.0.0</SW-VERSION>
                      <USED-CODE-GENERATOR>generators::generic::LwIp::main</USED-CODE-GENERATOR>
                      <VENDOR-ID>60</VENDOR-ID>
                      <AR-RELEASE-VERSION>4.0.3</AR-RELEASE-VERSION>
                      <VENDOR-SPECIFIC-MODULE-DEF-REFS>
                        <VENDOR-SPECIFIC-MODULE-DEF-REF DEST="ECUC-MODULE-DEF">/ArcCore/EcucDefs/Generic/LwIp</VENDOR-SPECIFIC-MODULE-DEF-REF>
                      </VENDOR-SPECIFIC-MODULE-DEF-REFS>
                    </BSW-IMPLEMENTATION>
                  </ELEMENTS>
                </AR-PACKAGE>
              </AR-PACKAGES>
            </AR-PACKAGE>
          </AR-PACKAGES>
        </AR-PACKAGE>
      </AR-PACKAGES>
    </AR-PACKAGE>
  </AR-PACKAGES>
</AUTOSAR>
